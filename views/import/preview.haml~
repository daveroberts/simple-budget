%h1 This will be imported
%pre= Marshal.dump(@entries)
- form_tag(:action=>"create") do
  %div= hidden_field :entries, :entries, :value=>Marshal.dump(@entries)
  %div= submit_tag 'Submit these'
%table
  %tr
    %th Date
    %th Description
    %th Amount
    %th Charged To
    %th Shared Between
    %th Tag
  - for entry in @entries
    %tr
      %td= entry.date
      %td= entry.description
      %td= entry.amount
      %td= entry.charged_to.name
      %td= entry.shared_between
      %td= entry.tag == nil ? "" : entry.tag.name